  "description": "Uma API simples para registro de veiculos e proprietarios",
  "main": "server.js",
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testURL": "http://localhost:3000/",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/test-env-setup.js"
